<table class="table table-hover table-striped">
    <thead>
    <tr>
        <th scope="col" class="span1">Date</th>
        <th scope="col" class="span7">Event</th>
        <th class="span1"></th>
    </tr>
    </thead>
    <tbody>
        {% for event in eventsList %}
            <tr>
                <td >{{ event.date|date('d/m/y')  }}</td>
                <td >{{ event.title }}</td>
                <td><a class="btn" href="#">Read More</a></td>
            </tr>
        {% endfor %}
    </tbody>
</table>